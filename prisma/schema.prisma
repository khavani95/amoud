datasource db {
  provider = "sqlite"   // برای شروع SQLite
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id        Int    @id @default(autoincrement())
  title     String
  shortDesc String
  fullDesc  String
  images    String // آدرس‌ها رو JSON یا CSV ذخیره کن
}

